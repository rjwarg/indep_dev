{{extend 'layout.html'}}
<h2>
    Active cases
</h2>
{{=A('Create New Case', _class='btn', _href=(URL('show_ajax)))}}
<table>

{{for row in rows:}}
    <tr>
        <td>{{=row.id}}</td>
        <td>{{=A(row.case_number, _href=(URL('edit_case', args=(row.id, row.case_number))))}}</td>
        <td>{{=row.member_id.last_name}}</td> 
        <td>{{=row.member_id.first_name}}</td>
        <td>{{=row.description}}</td>
    </tr>
{{pass}}
</table>
